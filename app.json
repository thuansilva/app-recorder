{
  "expo": {
    "name": "recorder-app",
    "slug": "airbnb-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "recorderapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.annathais.airbnbclone"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "process.env.GOOGLE_MAPS_API_KEY"
        }
      },
      "package": "com.annathais.airbnbclone"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Monstserrat-Regular.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Monstserrat",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Monstserrat-Regular.ttf",
                    "weight": "normal",
                    "style": "normal",
                    "fontFamily": "Monstserrat-Regular"
                  },
                  {
                    "path": "./assets/fonts/Monstserrat-Bold.ttf",
                    "weight": "bold",
                    "style": "normal",
                    "fontFamily": "Monstserrat-Bold"
                  },
                  {
                    "path": "./assets/fonts/Monstserrat-semibold.ttf",
                    "weight": "700",
                    "style": "normal",
                    "fontFamily": "Monstserrat-Semibold"
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/Monstserrat-Regular.ttf"
            ]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "28f222f5-5dc8-4915-b08f-8f36e95f53ab"
      }
    },
    "owner": "annathais"
  }
}
